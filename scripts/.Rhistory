# Función para calcular la moda
modaYa <- function(x) {
x <- na.omit(x)                 # quitar NA
ux <- unique(x)                 # valores únicos
ux[which.max(tabulate(match(x, ux)))]  # valor más frecuente
}
# Aplicar la función a cada columna
modasYa <- sapply(datosya, modaYa)
# Mostrar resultados
modasYa
# Pasar el resultado de modas a un data.frame
modasYa_df <- data.frame(
Anio = names(modasYa),
Valor_mas_frecuente = modasYa
)
# Guardar en CSV
write.csv(modasYa_df, "modasYa.csv", row.names = FALSE)
